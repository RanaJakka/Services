<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rxj8934">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".recyclerview.service.MyApplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recyclerview.service.LauncherActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"></category>
                <action android:name="android.intent.action.MAIN"></action>
            </intent-filter>
        </activity>
        <activity android:name=".recyclerview.service.recycler.RecyclerActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"></category>
                <action android:name="android.intent.action.MAIN"></action>
            </intent-filter>
        </activity>
        <activity android:name=".recyclerview.service.services.BoundService"></activity>
        <activity android:name=".recyclerview.service.services.IntentSeriviceActivity"></activity>
        <activity android:name=".recyclerview.service.fragments.MyFragmentActivity"></activity>
        <service android:name=".recyclerview.service.services.LongRunningService"></service>
        <service android:name=".recyclerview.service.services.MyIntentServices"></service>
        <activity android:name=".recyclerview.service.handlers.example1.HandlerActivity"></activity>


    </application>

</manifest>